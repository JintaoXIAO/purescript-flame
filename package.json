{
        "name": "purescript-flame",
        "version": "0.1.0",
        "files": [
                "package.json"
        ],
        "scripts": {
                "example-affjax": "pulp browserify --main Examples.Affjax.Main --include examples/Affjax --optimise --to examples/Affjax/dist/affjax.js",
                "example-counter": "pulp browserify --main Examples.Counter.Main --include examples/Counter --optimise --to examples/Counter/dist/counter.js",
                "example-counters": "pulp browserify --main Examples.Counters.Main --include examples/Counters:examples/Counter --optimise --to examples/Counters/dist/counters.js",
                "example-dice": "pulp browserify --main Examples.Dice.Main --include examples/Dice --optimise --to examples/Dice/dist/dice.js",
                "example-dices": "pulp browserify --main Examples.Dices.Main --include examples/Dices:examples/Dice --optimise --to examples/Dices/dist/dices.js",
                "build-examples": "npm run example-affjax && npm run example-counter && npm run example-counters && npm run example-dice && npm run example-dices"
        },
        "dependencies": {
                "phantomjs-prebuilt": "^2.1.16",
                "slimerjs": "^1.0.0",
                "snabbdom": "^0.7.3",
                "snabbdom-to-html": "^5.1.1"
        },
        "devDependencies": {
                "jsdom": "^14.0.0",
                "pulp": "^12.3.0"
        }
}
